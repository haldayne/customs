{
    "name": "haldayne/customs",
    "description": "Iterator for the $_FILES super-global, and a robust API for validating file uploads.",
    "license": "MIT",
    "authors": [
        {
            "name": "Bishop Bettini", "email": "bishop@php.net"
        }
    ],
    "require": {
        "php": "^5.5.0 || ^7.0",
        "haldayne/boost": "^1.0"
    },
    "autoload": {
        "classmap": [ "src" ],
        "psr-4": {
            "Haldayne\\Customs\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "^1.5",
        "mikey179/vfsStream": "^1.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Haldayne\\Customs\\": "tests/"
        }
    }
}
