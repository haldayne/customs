{
    "name": "haldayne/upload-iterator",
    "description": "Safe, simple iterator for the $_FILES super-global.",
    "license": "MIT",
    "authors": [
        {
            "name": "Bishop Bettini", "email": "bishop@php.net"
        }
    ],
    "require": {
        "php": "^5.5.0",
        "haldayne/boost": "1.0.x-dev"
    },
    "autoload": {
        "classmap": [ "src" ],
        "psr-4": {
            "Haldayne\\UploadIterator\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Haldayne\\UploadIterator\\": "tests/"
        }
    }
}
